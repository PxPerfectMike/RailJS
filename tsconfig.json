{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"lib": ["ES2015", "ES2020", "DOM"],
		"moduleResolution": "node",
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": ["type-check.test.ts"],
	"exclude": ["node_modules"]
}
